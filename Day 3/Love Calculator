print("Welcome to Love Calculator")
name1=input("Enter your name?")
name2=input("Enter his/her name?")
names=name1+name2
names.lower()
t=names.count('t')
r=names.count('r')
u=names.count('u')
e=names.count('e')
true=t+r+u+e
l=names.count('l')
o=names.count('o')
v=names.count('v')
e=names.count('e')
love=l+o+v+e
total=int(str(true)+str(love))
if total<10 or total>90:
  print(f"Your love score is {total}, You can go together like coke and mentos.")
elif total>=40 and total<=50:
  print(f"Your love score is {total}, You are alright together ")
else:
  print(f"Your love score is {total}")
